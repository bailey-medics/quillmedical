{
  "name": "public-pages",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "pages:gen": "node scripts/generate-pages.cjs",
    "dev": "yarn pages:gen && concurrently -k \"chokidar 'src/pages/*.tsx' -c 'yarn pages:gen'\" \"vite --open /index.html\"",
    "build": "yarn pages:gen && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@mantine/core": "^8.3.1",
    "@mantine/hooks": "^8.3.1",
    "@mantine/notifications": "^8.3.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.0",
    "vite": "^7.1.2",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
