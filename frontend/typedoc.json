{
  "tsconfig": "./tsconfig.app.json",
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "plugin": ["typedoc-plugin-markdown"],
  "out": "../docs/frontend",
  "readme": "none",
  "excludePrivate": true,
  "excludeInternal": true,
  "exclude": ["**/*.stories.*", "**/*.test.*", "**/*.spec.*"],
  "hideBreadcrumbs": true
}
